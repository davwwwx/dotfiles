Host example
    HostName example.com
    Port 22
    User example-user
    IdentityFile ~/.ssh/example-user/id_rsa

Host example2
    HostName example.org
    Port 1234
    User example2-user
    IdentityFile ~/.ssh/example2-user/id_rsa

Host github-project1
    User git
    HostName github.com
    IdentityFile ~/.ssh/github.project1.key

Host github-org
    User git
    HostName github.com
    IdentityFile ~/.ssh/github.org.key

Host github.com
    User git
    IdentityFile ~/.ssh/github.key
